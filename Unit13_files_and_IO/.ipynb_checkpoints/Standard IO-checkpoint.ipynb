    "#!/usr/bin/env python3\n",
    "\"Demonstrates stdio by echoing\"\n",
    "import sys\n",
    "while True:\n",
    "    line = sys.stdin.readline()\n",
    "    if line == '': # ctrl+D\n",
    "        break\n",
    "    sys.stdout.write(line)"
